{
  "name": "etfbl-fam",
  "expo": {
    "name": "etfbl-fam",
    "slug": "etfbl-fam",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "fam",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-logo-all.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F2EF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "org.unibl.etf.fam",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-android.png",
        "backgroundColor": "#F6F2EF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA8RffsG7SHWK6AKGVRpgkajNYKTuHE3sU"
        }
      },
      "splash": {
        "image": "./assets/images/splash-logo-android.png",
        "resizeMode": "contain",
        "backgroundColor": "#F6F2EF"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon-web.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-localization",
      "expo-image-picker",
      "expo-camera"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
